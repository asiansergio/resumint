{
  "name": "resumint",
  "version": "1.0.0",
  "description": "Generate multilingual resumes from JSON data",
  "main": "./src/main.ts",
  "type": "module",
  "bin": {
    "resumint": "./built/main.js"
  },
  "scripts": {
    "start": "node ./dist/main.js",
    "build": "tsc",
    "demo": "node ./dist/main.js --data ./data/example-data.json --html",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "keywords": [
    "resume",
    "cv",
    "generator",
    "pdf",
    "multilingual"
  ],
  "author": "Sergio Y. Lee",
  "license": "MIT",
  "packageManager": "pnpm@10.15.1",
  "dependencies": {
    "handlebars": "4.7.8",
    "nspell": "2.1.5",
    "puppeteer": "24.4.0",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "8.44.0",
    "@typescript-eslint/parser": "8.44.0",
    "eslint": "8.44.0",
    "eslint-config-prettier": "10.1.5",
    "prettier": "3.0.3",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
