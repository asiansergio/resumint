{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": ["error", "never"],
    "quotes": ["error", "double"],
    "no-use-before-define": ["error", { "functions": false }]
  }
}
